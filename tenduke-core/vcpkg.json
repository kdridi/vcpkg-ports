{
  "name": "tenduke-core",
  "version": "2.0.1",
  "description": "10Duke C++ Core - Core libraries for 10Duke C++ Enterprise Client",
  "homepage": "https://gitlab.com/10Duke/core/cpp-core",
  "license": "MIT",
  "default-features": ["static"],
  "features": {
    "static": {
      "description": "Build static libraries (required on Windows)"
    },
    "tests": {
      "description": "Build with tests",
      "dependencies": [
        "gtest"
